{
  "name": "hardhat-project",
  "scripts": {
    "start": "npx hardhat node",
    "ignition": "npm run compile && npm run deploy && (npm run exportArtifacts & npm run exportAccounts)",
    "compile": "npx hardhat compile",
    "exportArtifacts": "node scripts/exportArtifacts.js",
    "exportAccounts": "npx hardhat run scripts/exportAccounts.js --network hardhat",
    "deploy": "npx hardhat run scripts/deploy.js --network hardhat",
    "test": "npx hardhat test"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "hardhat": "^2.26.2"
  },
  "overrides": {
    "cookie": ">=0.7.0",
    "tmp": ">=0.2.4",
    "elliptic": ">=6.6.1",
    "undici": ">=5.28.6",
    "brace-expansion": ">=2.0.2",
    "glob": ">=10.0.0",
    "inflight": "npm:@isaacs/inflight@^1.0.1",
    "@nomicfoundation/hardhat-ethers": {
      "lodash.isequal": "npm:fast-deep-equal@^3.1.3"
    }
  }
}
